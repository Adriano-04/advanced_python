import unittest

from ..binary_search import binary_search

class TestBinary(unittest.TestCase):
    def test_binary(self):
        array = [1,2,4,5,7,8,9]
        find = 7

        result = binary_search(array, find, 0, len(array)-1)
        self.assertEqual(result, 5)

if __name__ == '__main__':
    unittest.main()